{
  "name": "WebHelpers",
  "update_url": "http://clients2.google.com/service/update2/crx",
  "version": "0.2",
  "manifest_version":2,
  "description": "常用WEB开发工具集",
  "background": {
      "scripts": [ "/background.js" ]
   },
   "browser_action": {
      "default_icon": "ico.png"
   },
  "permissions": [
    "tabs",
    "webRequest","webRequestBlocking",
    "http://*/*",
    "https://*/*"
  ]
}